{
  "name": "next-env-playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:run": "next dev",
    "dev:build": "next build",
    "dev:start": "next start",
    "stg:run": "NEXT_PUBLIC_ENV=stg next dev",
    "stg:build": "NEXT_PUBLIC_ENV=prod next dev",
    "stg:start": "NEXT_PUBLIC_ENV=stg next start",
    "lint": "next lint",
    "docker:build": "docker build -t env-playground -f Dockerfile .",
    "docker:run": "docker run -d --name env-playground -p 3000:3000 env-playground:latest"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "next": "^14.2.26",
    "next-runtime-env": "^1.8.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
